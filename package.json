{
  "name": "api-common-responses",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run --mode='test_local'",
    "test:watch": "vitest watch --mode='test_local'",
    "test:coverage": "vitest run --coverage --mode='test_local'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "vite": "^5.1.0",
    "vitest": "^1.2.2",
    "vitest-fetch-mock": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czargutz/api-common-responses.git"
  },
  "keywords": [
    "request",
    "homologate",
    "api",
    "fetch",
    "js",
    "react"
  ],
  "license": "ISC",
  "author": "Cesar Gutierrez Espitia, Jesus Francisco Garcia Esparza",
  "description": "Library to standardize API responses for easy control from the frontend",
  "homepage": "https://github.com/czargutz/api-common-responses#readme",
  "main": "./dist/request.umd.js",
  "module": "./dist/request.es.js",
  "bugs": {
    "url": "https://github.com/czargutz/api-common-responses/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/request.es.js",
      "require": "./dist/request.umd.js"
    }
  }
}
